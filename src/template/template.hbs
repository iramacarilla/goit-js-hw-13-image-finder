{{#each this}}
<li class= 'list'>
  <div>
  <div class="image-container">
        <img class="image" src="{{webformatURL}}" width="500px"  data-source={{largeImageURL}} alt="{{tags}}" />
    </div>

  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{downloads}}
    </p>
  </div>
</div>
</div>
</li>
{{/each}}
